{
  "name": "@ignisia/encryption",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "version": "0.1.3",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "types": "./dist/esm/*.d.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    },
    "./**/*": {
      "types": "./dist/esm/**/*.d.ts",
      "import": "./dist/esm/**/*.js",
      "require": "./dist/cjs/**/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "bun run build.ts"
  },
  "devDependencies": {
    "@ignisia/utils": "workspace:*",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krsbx/ignisia.git"
  },
  "bugs": {
    "url": "https://github.com/krsbx/ignisia/issues"
  },
  "keywords": [
    "bun",
    "crypto",
    "cryptography",
    "encryption",
    "decryption"
  ]
}
