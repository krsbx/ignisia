{
  "name": "@ignisia/replica",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "version": "0.1.5",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "types": "./dist/esm/*.d.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    },
    "./**/*": {
      "types": "./dist/esm/**/*.d.ts",
      "import": "./dist/esm/**/*.js",
      "require": "./dist/cjs/**/*.js"
    },
    "./package.json": "./package.json",
    "./primary": {
      "types": "./dist/esm/primary/index.d.ts",
      "import": "./dist/esm/primary/index.js",
      "require": "./dist/cjs/primary/index.js"
    },
    "./replica": {
      "types": "./dist/esm/replica/index.d.ts",
      "import": "./dist/esm/replica/index.js",
      "require": "./dist/cjs/replica/index.js"
    }
  },
  "scripts": {
    "build": "bun run build.ts"
  },
  "dependencies": {
    "@ignisia/encryption": "workspace:*"
  },
  "devDependencies": {
    "@ignisia/nosql": "workspace:*",
    "@ignisia/securedb": "workspace:*",
    "@ignisia/sql": "workspace:*",
    "@ignisia/utils": "workspace:*",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krsbx/ignisia.git"
  },
  "bugs": {
    "url": "https://github.com/krsbx/ignisia/issues"
  },
  "keywords": [
    "bun",
    "sql",
    "nosql",
    "database",
    "primary",
    "replica",
    "replicator",
    "replication"
  ]
}
